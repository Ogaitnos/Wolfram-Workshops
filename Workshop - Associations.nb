  RowBox[{"<|", 
   RowBox[{
    RowBox[{"a", "\[Rule]", "3"}], ",", 
    RowBox[{"b", "\[Rule]", "4"}], ",", 
    RowBox[{"c", "\[Rule]", "6"}]}], "|>"}]}]], "Input",
